{
  "name": "mx-mobile-widget-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "tsc",
    "generate:post_message_types": "ts-node src/post_messages/generate_types.ts",
    "generate:post_message_payloads": "ts-node src/post_messages/generate_payloads.ts",
    "generate:post_message_callbacks": "ts-node src/post_messages/generate_callbacks.ts",
    "generate": "npm run generate:post_message_types && echo && npm run generate:post_message_payloads && echo && npm run generate:post_message_callbacks"
  },
  "dependencies": {
    "expo": "~44.0.2",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-base64": "^0.2.1",
    "react-native-config": "^1.4.5",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-native-base64": "^0.2.0",
    "@types/yaml": "^1.9.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "yaml": "^1.10.2"
  },
  "private": true
}
